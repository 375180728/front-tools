  "main": "index.js",
  "scripts": {
    "dev-pc": "cross-env webpack-dev-server --mode development --config webpack.dev-pc.js",
    "dev-mobile": "cross-env webpack-dev-server --mode development --config webpack.dev-mobile.js",
    "build": "cross-env npm run clean && npm run build-pc && npm run build-mobile",
    "build-pc": "webpack --mode production --config webpack.production-pc.js",
    "build-mobile": "webpack --mode production --config webpack.production-mobile.js",
    "clean": "cross-env rm -rf dist && mkdir dist",
    "server": "supervisor server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "analyzer": "webpack --mode production --config webpack.production-mobile.js --profile --json > stats.json"
  },
